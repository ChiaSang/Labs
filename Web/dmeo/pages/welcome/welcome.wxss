page {
  height: 100%;
  background-color: black;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.avatar {
  border-radius: 50%;
  height: 200rpx;
  width: 200rpx;
  margin-top: 200rpx; 
}
.name {
  color: #f0f0f0;
  margin-top: 200rpx;
  font-size: 35px;
  font-weight: bold;
}
.start {
  margin-top: 240rpx;
  color: #f0f0f0;
  padding: 25rpx;
  border-radius: 15rpx;
  border: 2px solid #f7d614;
}